<div class="page">
  <header class="header js-header">
    <div class="container">
      <div class="d-flex align-items-center position-relative">
        <a href="/" class="logo">
          <img src="" alt="">
          <div>

          </div>
        </a>
        <a class="header-menu-btn">
          <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-menu"><line x1="3" y1="12" x2="21" y2="12"></line><line x1="3" y1="6" x2="21" y2="6"></line><line x1="3" y1="18" x2="21" y2="18"></line></svg>
        </a>
        <nav class="mr-auto header-nav d-none d-md-block">
          <a href="/users/sign_in" class="btn btn-sm"><i class="fe fe-log-in" style='vertical-align: -5px;'></i> <%=t :login%></a>
        </nav>
      </div>
    </div>
  </header>
  <header class="header header-sticky js-header">
    <div class="container">
      <div class="d-flex align-items-center position-relative">
        <a href="/" class="logo header-sticky">
          <img src="/img/tabler.svg" alt="Tabler UI Kit">
          <div>
            tabler
          </div>
        </a>
        <a class="header-menu-btn">
          <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-menu"><line x1="3" y1="12" x2="21" y2="12"></line><line x1="3" y1="6" x2="21" y2="6"></line><line x1="3" y1="18" x2="21" y2="18"></line></svg>
        </a>
        <nav class="ml-auto header-nav d-none d-md-block">
          <a href="https://preview.tabler.io">Demo</a>
          <a href="https://github.com/tabler/tabler">Source code</a>
          <a href="/emails" class="btn btn-sm">Tabler Email</a>
        </nav>
      </div>
    </div>
  </header>

  <main class="main">
    <section class="welcome" aria-label="Page header">
      <div class="container">
        <div class="row align-items-center">
          <div class="col-lg-6 text-center text-lg-left welcome-bg pr-lg-5">
            <h1 class="welcome-title"><%=t :build_your_tour_and_travel_website_and_app_instantly%></h1>
            <p class="welcome-description">
             <%=t :we_provide_saas_based_online_services_for_tur_and_travel_agencies%>
            </p>

            <div class="mt-5">
              <a href="/users/sign_up" class="btn btn-green" ><%=t :start%></a>
              <a href="http://demo.lvh.me:3000" class="btn btn-secondary ml-2" target="_blank"><%=t :demo%></a>
            </div>
          </div>
          <div class="col-lg-6 pt-6 pt-lg-0">
            <div class="welcome-image welcome-image-2">

              <div class="device device-macbook-pro device-spacegray">
                <div class="device-frame">
                  <picture>
                    <img src="<%= asset_url('screen.png')%>" alt="Preview of tabler dashboard" class="preview-image img-responsive">
                  </picture>
                </div>
                <div class="device-stripe"></div>
                <div class="device-header"></div>
                <div class="device-sensors"></div>
                <div class="device-btns"></div>
                <div class="device-power"></div>
              </div>

              <div style="bottom: 15%; left: 3%" class="image-label image-label-bottom d-none d-xl-block"><span>For free!</span></div>
              <div style="top: 20%; right: 3%" class="image-label image-label-right d-none d-xxl-block"><span>Look great in every browser</span></div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <section class="section" aria-label="Base features">
      <div class="container">
        <h2 class="h3 section-title text-center mb-6"><%=t :we_provide_you_with_lots_of_features%><br /><div class="text-muted small"><%=t :all_of_them_with_minimum_costs%></div></h2>

        <div class="row align-items-center justify-content-center">
          <div class="col-10 col-md-5 pr-md-5 mb-5 mb-md-0">
            <img src="/img/tabler-features.png" alt="" class="img-responsive">
          </div>
          <div class="col">

            <div class="row">

              <div class="col-12 mb-5 d-flex">
                <div class="icon icon-gray mr-2">
                  <i class="fe fe-box" style="font-size: 1.4rem;"></i>
                </div>
                <div>
                  <h3 class="h5"><%=t :ability_to_submit_multiple_packages%></h3>
                  <p class="text-muted m-0 text-justify">
                    <%=t :package_landing_details%>
                  </p>
                </div>
              </div>

              <div class="col-12 mb-5 d-flex">
                <div class="icon icon-gray mr-2">
                  <i class="fa fa-plane" style="font-size: 1.4rem;"></i><br />
                  <i class="fa fa-train" style="font-size: 1.4rem;"></i>
                  <i class="fa fa-bus" style="font-size: 1.4rem;"></i>
                </div>
                <div>
                  <h3 class="h5"><%=t :ability_to_submit_multiple_carriers%></h3>
                  <p class="text-muted m-0 text-justify">
                    <%=t :carrier_landing_details%>
                  </p>
                </div>
              </div>

              <div class="col-12 mb-5 d-flex">
                <div class="icon icon-gray mr-2">
                  <i class="fa fa-bed" style="font-size: 1.4rem;"></i>
                </div>
                <div>
                  <h3 class="h5"><%=t :ability_to_submit_multiple_hotels%></h3>
                  <p class="text-muted m-0 text-justify">
                    <%=t :hotel_landing_details%>
                  </p>
                </div>
              </div>

              <div class="col-12 mb-5 d-flex">
                <div class="icon icon-gray mr-2">
                  <i class="fe fe-shopping-cart" style="font-size: 1.4rem;"></i>
                  <i class="fa fa-money" style="font-size: 1.4rem;"></i>
                </div>
                <div>
                  <h3 class="h5"><%=t :ability_to_submit_multiple_prices%></h3>
                  <p class="text-muted m-0  text-justify">
                    <%=t :price_landing_details%>
                  </p>
                </div>
              </div>

            </div>
          </div>
        </div>

        <!--<div class="mb-xl-4 text-center text-lg-left">-->
        <!--<a href="#" class="btn btn-white">See all features</a>-->
        <!--</div>-->
      </div>
    </section>

    <section class="section bg-light">
      <div class="container text-center">
        <h2 class="section-title mb-2"><%=t :ready_to_start_?%></h2>
        <p class="section-description">
          <%=t :start_easily_now%>
          <div class="mt-4">
            <a href="/users/sign_up" class="btn btn-green" ><%=t :start%></a>
            <a href="http://demo.lvh.me:3000" class="btn btn-secondary ml-2" target="_blank"><%=t :demo%></a>
          </div>
        </div>
      </section>




      <div class="modal js-modal hidden" style="display:none;">
        <div class="modal-overlay"></div>
        <div class="modal-content">
          <a href="#" class="modal-close js-modal-close">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
          </a>
          <a href="/emails/">
            <img src="/img/thumbs/promo-800.png" srcset="/img/thumbs/promo-800@2x.png 2x" alt="" class="modal-content-image">
          </a>
        </div>
      </div>

    </main>

    <footer class="footer">
      <div class="container">
        <div class="row  text-lg-left">
          <div class="col-lg-12 text-right">
            <p><%=t :developed_by_bitgarden%></p>
          </div>
        </div>
      </div>
    </footer>

  </div>
